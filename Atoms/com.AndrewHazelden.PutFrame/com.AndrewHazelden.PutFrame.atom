Atom {
	Name = "PutFrame",
	Category = "Tools/IO",
	Author = "Andrew Hazelden",
	Version = 1.1,
	Date = {2019, 10, 5},
	Description = [[<p>PutFrame.fuse allows you to save RGBA channel based BMP/GIF/JPEG/PNG/TGA/TIFF/ imagery to disk using the Fuse API's Clip:PutFrame() functionality. This fuse can be used as a learning tool should you need to re-create Saver node like approaches in your own fuses.</p>
	
<p>This fuse is compatible with Fusion Standalone 16+ and Resolve 16+.</p>]],
	Deploy = {
		"Fuses/IO/PutFrame.fuse",
	},
}
